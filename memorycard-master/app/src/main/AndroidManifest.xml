<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.memorycardgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_memorycard"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_memorycard_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MemoryCardGame">
        <activity android:name=".scoresActivity"></activity>
        <activity android:name=".aide" />
        <activity android:name=".playActHard" />
        <activity android:name=".playActMeduim" />
        <activity android:name=".playactivity" />
        <activity
            android:name=".MainActivity"
            android:label="MEMORY CARD GAME">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>